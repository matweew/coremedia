<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="CmsSlotData">
        <property name="content" type="string"/>
    </transfer>

    <transfer name="CmsSlotContentRequest">
        <property name="cmsSlotKey" type="string"/>
        <property name="params" type="array"/>
    </transfer>

    <transfer name="CoreMediaApiResponse">
        <property name="isSuccessful" type="bool"/>
        <property name="data" type="string"/>
    </transfer>

    <transfer name="CoreMediaFragmentRequest">
        <property name="store" type="string"/>
        <property name="locale" type="string"/>
        <property name="productId" type="string"/>
        <property name="categoryId" type="string"/>
        <property name="pageId" type="string"/>
        <property name="placement" type="string"/>
        <property name="view" type="string"/>
    </transfer>

    <transfer name="Store">
        <property name="name" type="string"/>
    </transfer>

</transfers>